{
  "name": "tunemyles-be-v2",
  "version": "2.0.0",
  "description": "Back-End for Tunemyles Project",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node",
    "dev": "concurrently \"npm run nodemon\" \"npm run prettier-watch\"",
    "nodemon": "nodemon src/index.ts",
    "prettier-watch": "onchange \"**/*\" -- prettier --write --ignore-unknown {{changed}}",
    "test": "jest",
    "lint": "eslint src",
    "compile-schemas": "json2ts -i src/schemas -o src/types",
    "yaml2json": "yaml2json",
    "json2yaml": "json2yaml"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fikryfahrezy/tunemyles-be-v2.git"
  },
  "author": "frz",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fikryfahrezy/tunemyles-be-v2/issues"
  },
  "homepage": "https://github.com/fikryfahrezy/tunemyles-be-v2#readme",
  "devDependencies": {
    "@babel/core": "7.12.13",
    "@babel/preset-env": "7.12.13",
    "@babel/preset-typescript": "7.12.13",
    "@tsconfig/recommended": "1.0.1",
    "@types/busboy": "0.2.3",
    "@types/jest": "26.0.20",
    "@types/node": "14.14.25",
    "@types/pump": "1.1.0",
    "@types/supertest": "2.0.10",
    "@typescript-eslint/eslint-plugin": "4.14.2",
    "@typescript-eslint/parser": "4.14.2",
    "babel-jest": "26.6.3",
    "concurrently": "5.3.0",
    "eslint": "7.19.0",
    "eslint-config-prettier": "7.2.0",
    "form-auto-content": "2.1.0",
    "husky": "5.0.9",
    "jest": "26.6.3",
    "json-schema-to-typescript": "10.1.3",
    "lint-staged": "10.5.4",
    "nodemon": "2.0.7",
    "onchange": "7.1.0",
    "prettier": "2.2.1",
    "ts-node": "9.1.1",
    "typescript": "4.1.3",
    "yamljs": "0.3.0"
  },
  "dependencies": {
    "bcrypt": "5.0.0",
    "cls-hooked": "4.2.2",
    "dotenv": "8.2.0",
    "fastify": "3.11.0",
    "fastify-cors": "5.2.0",
    "fastify-helmet": "5.2.0",
    "fastify-multipart": "4.0.0",
    "fastify-plugin": "3.0.0",
    "fastify-static": "3.4.0",
    "fastify-swagger": "4.3.0",
    "jsonwebtoken": "8.5.1",
    "make-promises-safe": "5.1.0",
    "md5": "2.3.0",
    "middie": "5.2.0",
    "mysql2": "2.2.5",
    "nodemailer": "6.4.18",
    "pump": "3.0.0",
    "sequelize": "6.5.0",
    "slugify": "1.4.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js}": "eslint --cache --fix",
    "*.{ts,js,css,md}": "prettier --write"
  }
}
